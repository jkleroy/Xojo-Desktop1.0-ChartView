		Sub GetChartPicture(Row As Integer, Column As Integer, Type As Integer, Pic As Picture)
		  Dim Data As String = me.cell(row, column)
		  
		  ChartView4.Freeze = True
		  ChartView4.LoadFromCSV(Data, True, ",", False, True)
		  ChartView4.Series(0).MarkerType = ChartSerie.MarkerNone
		  ChartView4.Axes(1).MinimumScale = 0
		  ChartView4.Series(0).FillColor = &c0
		  ChartView4.Series(0).LineWeight = 2
		  
		  #if TargetWin32
		    Dim lastGDI As Boolean = App.UseGDIPlus
		    App.UseGDIPlus = True
		  #endif
		  
		  Pic.Mask.Graphics.ForeColor = &cFFFFFF
		  Pic.Mask.Graphics.AAG_FillAll()
		  ChartView4.ExportGraph(Pic.Mask, Type)
		  
		  Pic.Graphics.ForeColor = ChartView4.DefaultColors(Row mod ChartView4.DefaultColors.Ubound)
		  Pic.Graphics.AAG_FillAll
		  
		  #if TargetWin32
		    App.UseGDIPlus = lastGDI
		  #endif
		End Sub
