		  me.LegendPosition = Chartview.Position.Right
		  
		  me.HighlightMinValue(&cFF0000, -1)
		  
		  me.Freeze = False
		  me.Animate = True
		  me.Redisplay()
		  'me.StartAnimation(True, ChartAnimate.kEaseOutCirc)
		End Sub
