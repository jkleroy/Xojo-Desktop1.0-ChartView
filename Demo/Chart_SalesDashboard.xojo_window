#tag Events ChartView2
	#tag Event
		Sub Open()
		  me.Freeze = True
		  
		  'ChartView2.BackgroundType = me.BackgroundGradient
		  'ChartView2.BackgroundColor.Append &cFAFAFA
		  'ChartView2.BackgroundColor.Append &cE6E6E6
		  
		  
		  me.Title = "REVENUE by PRODUCT TYPE ($thousand)"
		  me.Type = me.TypeCombo
		  
		  me.CrossesBetweenTickMarks = True
		  
		  Dim S As ChartSerie
		  
		  S = New ChartSerie
		  S.Title = "actual"
		  S.Type = me.TypeColumn
		  S.FillType = S.FillPicture
		  S.Data = Array(1169, 1252, 1313, 1214, 1301, 1144, 1188, 1207, 1275, 1324, 1281, 1366)
		  S.LineWeight = 2
		  S.MarkerType = S.MarkerNone
		  me.AddSerie(S)
		  
		  S = New ChartSerie
		  S.Title = "target"
		  S.Type = me.TypeLine
		  S.FillColor = &c1B9DDE
		  
		  S.Data = Array(1150, 1250, 1300, 1200, 1250, 1300, 1330, 1330, 1250, 1300, 1330, 1350)
		  S.LineWeight = 2
		  S.MarkerType = S.MarkerSquare
		  'S.MarkerSize = 4
		  'S.SecondaryAxis = True
		  
		  me.AddSerie(S)
		  
		  Dim Cl As New ChartLine
		  Cl.Width = 2
		  Cl.FillColor = &cE4E4E4
		  me.Axes(1).MajorGridLine = Cl
		  
		  
		  me.LegendPosition = ChartView.Position.Right
		  
		  
		  Dim i As Integer
		  
		  Dim d As date = New Date
		  d.Day = 1
		  //Setting up labels
		  For i  = 1 to 12
		    d.Month = i
		    me.Axes(0).Label.append New Date(d)
		  Next
		  
		  me.Series(0).Calc
		  lblTotal.Text = str(me.Series(0).TotalValue, "\$ ###,##0.00") 
		  me.Series(1).Calc
		  Dim a As Double = me.Series(0).TotalValue
		  Dim b As Double = me.Series(1).TotalValue
		  If a<b then
		    lblTarget.TextColor = &cDD0000
		  else
		    lblTarget.TextColor = &c00DD00
		  End If
		  lblTarget.Text = str((a-b)/b, "-#.00 %")
		  
		  
		  me.Axes(0).AutoLabel = False
		  me.Axes(0).LabelStyle.Format = "M" ' ?yyyy"
		  me.setTransparency(40)
		  'me.Axes.Append New ChartAxis
		  'me.Axes(2).Title = "Test"
		  
		  
		  me.Freeze = False
		  me.Animate = True
		  me.StartAnimation
		End Sub
	#tag EndEvent
	#tag Event
		Sub GetFillPicture(ByRef p As Picture, Serie As ChartSerie, Index As Integer, Label As String, DrawX As Integer, DrawY As Integer)
		  //Here we define which color to use for the ColumnChart.
		  //When the value is smaller than the Target, the color is red
		  
		  #Pragma Unused DrawX
		  #Pragma Unused DrawY
		  
		  Dim g As Graphics
		  g = p.Graphics
		  
		  If Index = -1 then
		    If Label<> "Legend" then Return
		    
		    g.ForeColor = &cCC0000
		    g.AAG_FillAll()
		    g.ForeColor = &cADADAD
		    g.AAG_DrawLineAFill(0, g.Height-1, g.Width-1, 0, 1, True)
		    
		  else
		    
		    If Serie.Values(Index) >= me.Series(1).Values(Index) then
		      //Value is bigger than target
		      
		      g.ForeColor = &cADADAD
		      
		    else
		      //Value is smaller than target
		      
		      g.ForeColor = &cCC0000
		      
		      
		    End IF
		    
		    g.FillRect(0, 0, g.Width, g.Height)
		    
		  End If
		End Sub
	#tag EndEvent
