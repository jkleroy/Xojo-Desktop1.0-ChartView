		Sub Open()
		  me.Freeze = True
		  
		  me.Title = "REVENUE by PRODUCT TYPE ($thousand)"
		  me.Type = me.TypeLine
		  me.FollowValues = True
		  me.FollowAllSeries = True
		  
		  
		  Dim S As ChartSerie
		  
		  S = New ChartSerie
		  S.Title = "food"
		  S.Data = Array(428.0, 483.0, 520.0, 470.0,482.0,410.0,418.0,423.0,461.0,484.0,450.0,526.0)
		  S.LineWeight = 2
		  S.MarkerType = S.MarkerNone
		  S.Shadow = True
		  S.ShadowColor = &c000000C3
		  me.AddSerie(S)
		  
		  S = New ChartSerie
		  S.Title = "clothing"
		  S.Data = Array(333.0, 345.0, 351.0, 336.0, 366.0, 356.0, 365.0, 374.0, 398.0, 370.0, 352.0, 384.0)
		  S.LineWeight = 2
		  S.MarkerType = S.MarkerNone
		  S.Shadow = True
		  S.ShadowColor = &c000000C3
		  me.AddSerie(S)
		  
		  S = New ChartSerie
		  S.Title = "homeware"
		  S.Data = Array(307, 311, 350, 316, 331, 298, 286, 292,314, 339, 308, 352)
		  S.LineWeight = 2
		  S.MarkerType = S.MarkerNone
		  S.Shadow = True
		  S.ShadowColor = &c000000C3
		  me.AddSerie(S)
		  
		  
		  
		  Dim i As Integer
		  
		  Dim d As date = New Date
		  d.Day = 1
		  For i  = 1 to 12
		    
		    d.Month = i
		    me.Axes(0).Label.append New Date(d)
