		  #Pragma Unused X
		  #Pragma Unused Y
		  
		  Redim Me.BackgroundColor(-1)
		  Me.BackgroundColor.Append &c2d2d2d
		  Me.BackgroundType = Me.BackgroundSolid
		  
		  me.Axes(0).ForeColor = TextColor
		  
		  Me.Redisplay
		End Sub
