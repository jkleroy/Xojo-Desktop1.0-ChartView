#tag Events GoogleButton
	#tag Event
		Sub Action()
		  ChartView1.Type = ChartView.TypeArea
		  
		  
		  ChartView1.Freeze = True
		  ChartView1.DeleteAllMarkLines()
		  ChartView1.LoadFromCSV(Google_csv, False, ";")
		  
		  
		  ExportGraph(me)
		  
		  
		  ChartView1.Series(0).LineWeight = 2
		  ChartView1.Series(0).Transparency = 50
		  ChartView1.Series(0).MarkerType = ChartSerie.MarkerNone
		  ChartView1.Axes(0).LabelStyle.Format = "M" + EndOfLine + "?yyyy"
		  ChartView1.Axes(0).MajorUnit = 30
		  ChartView1.Axes(0).LabelInterval = 60
		  ChartView1.Axes(0).LabelStyle.TextSize = 12
		  ChartView1.Axes(0).Title = ""
		  ChartView1.Axes(1).LabelStyle.TextSize = 12
		  ChartView1.Axes(1).MinimumScale = 250
		  'ChartView1.Axes(1).LogarithmicScale = True
		  
		  Redim ChartView1.Marks(-1)
		  
		  Dim Avg As Single = ChartView1.Series(0).Average
		  ChartView1.AddMarkLine(Avg, str(Avg, "#,00") + " AVG Value", &c808080, 2)
		  
		  ChartView1.HighlightMaxValue(&c00DD00)
		  ChartView1.HighlightMinValue(&cDD0000)
		  
		  
		  
		  ChartView1.Freeze = False
		  ChartView1.Redisplay()
		End Sub
