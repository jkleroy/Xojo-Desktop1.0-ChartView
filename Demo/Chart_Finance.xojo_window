	#tag Method, Flags = &h21
		Private Sub ExportGraph(btn As FinanceButton)
		  Dim p As Picture
		  If ScalingFactor>1 then
		    p = new Picture(btn.Width*2, btn.Height*2, 32)
		  else
		    p = New Picture(btn.Width, btn.Height, 32)
		  End If
		  
		  Dim g As Graphics = p.Graphics
		  Dim PrevColor As Color = ChartView1.Series(0).FillColor
		  g.ForeColor = ChartView1.Series(0).FillColor
		  g.FillRect(0, 0, g.Width, g.Height)
		  ChartView1.Series(0).MarkerType = ChartSerie.MarkerNone
		  ChartView1.Axes(1).MinimumScale = 0
		  ChartView1.Series(0).FillColor = &cFFFFFF
		  ChartView1.Series(0).LineWeight = 2
		  
		  'Dim ms As Double = Microseconds()
		  
		  ChartView1.ExportGraph(p, ChartView1.TypeLine)
		  
		  ChartView1.Series(0).FillColor = PrevColor
		  
		  
		  'ms = (Microseconds-ms)/1000
		  g.ForeColor = &cFFFFFF
		  g.Bold = True
		  If ScalingFactor>1 then
		    g.textsize = 24
		    g.DrawString(btn.Caption, 4, 4 + g.textascent)
		  else
		    g.DrawString(btn.Caption, 2, 2+g.TextAscent)
		  End If
		  
		  btn.ChartPic = p
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Function ScalingFactor() As Single
		  #If TargetCocoa Then
		    Try
		      Soft Declare Function BackingScaleFactor Lib "AppKit" Selector "backingScaleFactor" (target As Integer) As Double
		      Return BackingScaleFactor(Self.Handle)
		    Catch e As ObjCException
		      Return 1
		    End Try
		  #Else
		    Return 1
		  #Endif
		End Function
	#tag EndMethod


