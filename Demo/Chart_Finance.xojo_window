		Sub Open()
		  me.FollowValues = True
		  'me.FollowAllSeries = True
		  
		  me.Axes(1).MajorGridLine = New ChartLine()
		  me.Axes(1).MajorGridLine.FillColor = &cE0E0E0
		  
		  me.Axes(0).AxeLine.Width = 2
		  me.Axes(0).AxeLine.FillType = ChartLine.FillNone
		  me.Axes(1).AxeLine.Width = 2
		  me.Axes(1).AxeLine.FillType = ChartLine.FillNone
		  
		  me.LegendPosition = ChartView.Position.None
